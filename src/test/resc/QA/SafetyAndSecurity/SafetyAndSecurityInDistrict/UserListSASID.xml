<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >


<suite name="مدير الامن والسلامو باردارة التعليم">

    <test name="موظف صلاحيات مدير الامن والسلامة المدرسية بادارة التعليم">
        <classes>
            <class name="NoorProject.Other.NoorLogin">
                <methods>
                    <include name="SecurityAndSafetyManagersInDistrictLogin"/>
                </methods>
            </class>
            <class name="NoorProject.EduWaveSafeAndSecurity.SecurityAndSafetyManagersInDistrict.UsersMenu.PrivilegeUser.UserAuthorityEmployee"/>
            <class name="NoorProject.Other.NoorLogOut"/>
        </classes>
    </test>
    <test name="موظفون بإدارة الأمن والسلامة بإدارة التعليم">
        <classes>
            <class name="NoorProject.Other.NoorLogin">
                <methods>
                    <include name="SecurityAndSafetyManagersInDistrictLogin"/>
                </methods>
            </class>
            <class name="NoorProject.EduWaveSafeAndSecurity.SecurityAndSafetyManagersInDistrict.UsersMenu.EmployeesInTheSecurityAndSafetyDepartment.StaffMember"/>
            <class name="NoorProject.Other.NoorLogOut"/>
        </classes>
    </test>

</suite>