<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >


<suite name="قائمة الاعدادت لدى مدير عام الأمن والسلامة المدرسية">

    <test name="أنواع الزيارات/اضافة">

        <classes>
            <class name="NoorProject.Other.NoorLogin">
                <methods>
                    <include name="GeneralDirectorOfSchoolSecurityAndSafety"/>
                </methods>
            </class>
            <class name="NoorProject.EduWaveSafeAndSecurity.GeneralDirectorOfSchoolSecurityAndSafety.Settings.ADDVistis"/>
            <class name="NoorProject.Other.NoorLogOut"/>
        </classes>

    </test>
    <test name="أنواع الزيارات/حذف">

        <classes>
            <class name="NoorProject.Other.NoorLogin">
                <methods>
                    <include name="GeneralDirectorOfSchoolSecurityAndSafety"/>
                </methods>
            </class>
            <class name="NoorProject.EduWaveSafeAndSecurity.GeneralDirectorOfSchoolSecurityAndSafety.Settings.DeleteVistis"/>
            <class name="NoorProject.Other.NoorLogOut"/>
        </classes>

    </test>
    <test name="أنواع الزيارات/نعديل">

        <classes>
            <class name="NoorProject.Other.NoorLogin">
                <methods>
                    <include name="GeneralDirectorOfSchoolSecurityAndSafety"/>
                </methods>
            </class>
            <class name="NoorProject.EduWaveSafeAndSecurity.GeneralDirectorOfSchoolSecurityAndSafety.Settings.EditVistis"/>
            <class name="NoorProject.Other.NoorLogOut"/>
        </classes>

    </test>

</suite>