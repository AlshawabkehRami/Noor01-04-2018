<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >


<suite name="نماذج السلامة المدرسية">

    <test name="أضافة نموذج الامن والسلامة">

        <classes>
            <class name="NoorProject.Other.NoorLogin">
                <methods>
                    <include name="GeneralDirectorOfSchoolSecurityAndSafety"/>
                </methods>
            </class>
            <class name="NoorProject.EduWaveSafeAndSecurity.GeneralDirectorOfSchoolSecurityAndSafety.EduWaveSafeAndSecurityForms.ADDSafetyForms"/>
            <class name="NoorProject.Other.NoorLogOut"/>


        </classes>

    </test>
    <test name="حذف نموذج الامن والسلامة">

        <classes>
            <class name="NoorProject.Other.NoorLogin">
                <methods>
                    <include name="GeneralDirectorOfSchoolSecurityAndSafety"/>
                </methods>
            </class>
            <class name="NoorProject.EduWaveSafeAndSecurity.GeneralDirectorOfSchoolSecurityAndSafety.EduWaveSafeAndSecurityForms.DeleteSafetyForms"/>
            <class name="NoorProject.Other.NoorLogOut"/>
        </classes>

    </test>

    <test name="عرض  الاستمارة/منشور">

        <classes>
            <class name="NoorProject.Other.NoorLogin">
                <methods>
                    <include name="GeneralDirectorOfSchoolSecurityAndSafety"/>
                </methods>
            </class>
            <class name="NoorProject.EduWaveSafeAndSecurity.GeneralDirectorOfSchoolSecurityAndSafety.EduWaveSafeAndSecurityForms.ViewTheFormPublished"/>
            <class name="NoorProject.Other.NoorLogOut"/>
        </classes>
    </test>
    <test name="عرض  الاستمارة/غير منشور">

        <classes>
            <class name="NoorProject.Other.NoorLogin">
                <methods>
                    <include name="GeneralDirectorOfSchoolSecurityAndSafety"/>
                </methods>
            </class>
            <class name="NoorProject.EduWaveSafeAndSecurity.GeneralDirectorOfSchoolSecurityAndSafety.EduWaveSafeAndSecurityForms.ViewTheFormUnPublished"/>
            <class name="NoorProject.Other.NoorLogOut"/>
        </classes>
    </test>
    <test name="عرض اقسام الاستمارة">

        <classes>
            <class name="NoorProject.Other.NoorLogin">
                <methods>
                    <include name="GeneralDirectorOfSchoolSecurityAndSafety"/>
                </methods>
            </class>
            <class name="NoorProject.EduWaveSafeAndSecurity.GeneralDirectorOfSchoolSecurityAndSafety.EduWaveSafeAndSecurityForms.ViewSectionsOfTheForm"/>
            <class name="NoorProject.Other.NoorLogOut"/>
        </classes>
    </test>

    <test name="عرض اقسام الاستمارة/البنود الخارجية">

        <classes>
            <class name="NoorProject.Other.NoorLogin">
                <methods>
                    <include name="GeneralDirectorOfSchoolSecurityAndSafety"/>
                </methods>
            </class>
            <class name="NoorProject.EduWaveSafeAndSecurity.GeneralDirectorOfSchoolSecurityAndSafety.EduWaveSafeAndSecurityForms.ExternalItems"/>
            <class name="NoorProject.Other.NoorLogOut"/>
        </classes>
    </test>

    <test name="عرض اقسام الاستمارة/البنود الخارجية/البنود الداخلية">

        <classes>
            <class name="NoorProject.Other.NoorLogin">
                <methods>
                    <include name="GeneralDirectorOfSchoolSecurityAndSafety"/>
                </methods>
            </class>
            <class name="NoorProject.EduWaveSafeAndSecurity.GeneralDirectorOfSchoolSecurityAndSafety.EduWaveSafeAndSecurityForms.InternalItems"/>
            <class name="NoorProject.Other.NoorLogOut"/>
        </classes>
    </test>

    <test name="تعديل نموذج امن وسلامة غير منشور">

        <classes>
            <class name="NoorProject.Other.NoorLogin">
                <methods>
                    <include name="GeneralDirectorOfSchoolSecurityAndSafety"/>
                </methods>
            </class>
            <class name="NoorProject.EduWaveSafeAndSecurity.GeneralDirectorOfSchoolSecurityAndSafety.EduWaveSafeAndSecurityForms.EditSafetyForms"/>
            <class name="NoorProject.Other.NoorLogOut"/>
        </classes>

    </test>

</suite>